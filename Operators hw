#include<stdio.h>
int arithmetic();
int relational();
int logical();
int bitwise();
int assignment();
int main(){
	arithmetic();
	relational();
	logical();
	bitwise();
	assignment();
}
int arithmetic(){
	int a = 11;
 int b = 10;
 int c ;
 printf("Arithmetic operator\n\n");
 printf("if a = 11 and b = 10\n");
 c = a + b;
 printf("Line 1 - Value of c is %d\n", c );
 c = a - b;
 printf("Line 2 - Value of c is %d\n", c );
 c = a * b;
 printf("Line 3 - Value of c is %d\n", c );
 c = a / b;
 printf("Line 4 - Value of c is %d\n", c );
 c = a % b;
 printf("Line 5 - Value of c is %d\n", c );
 c = a++;
 printf("Line 6 - Value of c is %d\n", c );
 c = a--;
 printf("Line 7 - Value of c is %d\n\n", c );
 return 0;
}
int relational(){
 int a = 15;
 int b = 15;
 int c ;
 printf("Relational operator\n\n");
 printf("if a = 15 and b = 15\n");
 if( a == b )
 {
 printf("Line 1 - a is equal to b\n" );
 }
 else
 {
 printf("Line 1 - a is not equal to b\n" );
 }
 if ( a < b )
 {
 printf("Line 2 - a is less than b\n" );
 }
 else
 {
 printf("Line 2 - a is not less than b\n" );
 }
 if ( a > b )
 {
 printf("Line 3 - a is greater than b\n" );
 }
 else
 {
 printf("Line 3 - a is not greater than b\n" );
 }
 /* Lets change value of a and b */
 a = 5;
 b = 20;
 if ( a <= b )
 {
 printf("Line 4 - a is either less than or equal to b\n" );
 }
 if ( b >= a )
 {
 printf("Line 5 - b is either greater than or equal to b\n\n" );
}
}
int logical(){
	printf("Logical operator\n\n");
	printf("if a = 10 and b = 5\n");
int a = 10;
 int b = 5;
 int c ;
 if ( a && b )
 {
 printf("Line 1 - Condition is true\n" );
 }
 if ( a || b )
 {
 printf("Line 2 - Condition is true\n" );
 }
 /* lets change the value of a and b */
 printf("if a = 0 and b = 5\n");
 a = 0;
 b = 5;
 if ( a && b )
 {
 printf("Line 3 - Condition is true\n" );
 }
 else
 {
 printf("Line 3 - Condition is not true\n" );
 }
 if ( !(a && b) )
 {
 printf("Line 4 - Condition is true\n\n" );
 }
}
int bitwise(){
	unsigned int a = 35; /* 35 = 0010 0011 */ 
 unsigned int b = 10; /* 10 = 0000 1010 */
 printf("Bitwise operator\n\n");
 printf("if a = 35 and b = 10\n");
 int c = 0; 
 c = a & b; 
 printf("Line 1 - Value of c is %d\n", c );
 c = a | b; 
 printf("Line 2 - Value of c is %d\n", c );
 c = a ^ b; 
 printf("Line 3 - Value of c is %d\n", c );
 c = ~a; 
 printf("Line 4 - Value of c is %d\n", c );
 c = a << 2; 
 printf("Line 5 - Value of c is %d\n", c );
 c = a >> 2; 
 printf("Line 6 - Value of c is %d\n\n", c );
}
int assignment(){
 int a = 11;
 int c ;
 printf("Assignment operator\n\n");
 printf("if a = 11\n");
 c = a;
 printf("Line 1 - = Operator Example, Value of c = %d\n", c );
 c += a;
 printf("Line 2 - += Operator Example, Value of c = %d\n", c );
 c -= a;
 printf("Line 3 - -= Operator Example, Value of c = %d\n", c );
 c *= a;
 printf("Line 4 - *= Operator Example, Value of c = %d\n", c );
 c /= a;
 printf("Line 5 - /= Operator Example, Value of c = %d\n", c );
 c = 200;
 c %= a;
 printf("Line 6 - %= Operator Example, Value of c = %d\n", c );
 c <<= 2;
 printf("Line 7 - <<= Operator Example, Value of c = %d\n", c );
 c >>= 2;
 printf("Line 8 - >>= Operator Example, Value of c = %d\n", c );
 c &= 2;
 printf("Line 9 - &= Operator Example, Value of c = %d\n", c );
 c ^= 2;
 printf("Line 10 - ^= Operator Example, Value of c = %d\n", c );
 c |= 2;
 printf("Line 11 - |= Operator Example, Value of c = %d\n", c );
}
